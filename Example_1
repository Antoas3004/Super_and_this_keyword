//parent class
class Parent_class {
	 int a=1;
	 
	 void m() {
		 System.out.println(a);
	 }
}
//child class
//chlid class extends parent class
class Child_Class extends Parent_class {

	int a = 2;

	void n() {
		int a = 3;
		System.out.println(a + this.a + super.a);
	}
}
//Main Class
public class Main_Class {

	public static void main(String[] args) {

		Child_Class obj = new Child_Class();
		obj.m();
		obj.n();

	}
}
//Super is a keyword to use,use the parent class instance variable 
//in child class
//this keyword is used for the object has same variable name then we
//use this keyword to seperate that vari
